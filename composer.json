{
    "name": "t4web/pages",
    "description": "ZF2 Module for easily creating static pages.",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2"
    ],
    "homepage": "https://github.com/t4web/Pages",
    "authors": [
        {
            "name": "Max Gulturyan",
            "email": "gulturyan@gmail.com",
            "homepage": "http://about.me/maxgu"
        }
